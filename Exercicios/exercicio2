package Fixacao;

public class exercicio2 {
/* Exercício 2: Crie um programa que leia dois números inteiros e imprima a soma
*/
	public static void main(String[] args) {
		
		int a = (int) 3;
		int b = (int) 4;
		int c = (int) 5;
		float media = (a + b + c)/3;
		System.out.println("O resultado da media é: " + media);
	}

}

